               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(written lines) specifies the number of written lines per column</a:documentation>
               <list>
                  <data type="nonNegativeInteger"/>
                  <optional>
                     <data type="nonNegativeInteger"/>
                  </optional>
               </list>
            </attribute>
         </optional>
         <empty/>
