            <optional>
               <ref name="teiavailability"/>
            </optional>
            <optional>
               <ref name="teicustodialHist"/>
            </optional>
            <optional>
               <ref name="teimodel.noteLike"/>
            </optional>
         </group>
         <ref name="teiatt.global.attributes"/>
         <empty/>
